   MV %5 ""
   IH%1;U50BB;
   MV #1 "CLEAR-FILE (DATA $STAT-FILE*"*%5*")"
   PH
   IF E # 201 GO 55
   MV #1 "CREATE-FILE ($STAT-FILE*"*%5*" 1,1 257,1) (X)"
   PH
   IF E # 417 T "Can not create Stat file "üRTN
   MV #1 "COPY DICT STAT-FILE *"
   STON
   MV #1 "(DICT ","$STAT-FILE*"*%5
   C NOW READ "D" POINTER WHILE THE FILE NAME IS AVAILABLE.
   F-CLEAR 2
   F-OPEN 2 MD
   T "CAN NOT OPEN MD "üRTN
   F-READ 2 #2
   T "CANNOT READ TEMP STAT FILE"üRTN
   H)<
   PH
   MV &2.1 "DX",,,,,,,,,
   F-WRITE 2
55 Start Save 
   HFORM-LIST SYSTEM-OBJECT FSAVE-LIST
   STON
   HPQ-SELECT 1
   P
33 MV %1 !1
   IF # %1 GO 44
   MV #1 "LOCK-FRAME ",%1
   PH
